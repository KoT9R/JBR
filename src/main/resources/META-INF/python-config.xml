<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <localInspection language="Python"
                         shortName="ParameterOfFunction"
                         suppressId="JBRTest"
                         groupName="JBR"
                         enabledByDefault="true"
                         level="WARNING"
                         implementationClass="ParameterInspection"
                         displayName="Add type"/>
        <localInspection language="Python"
                         shortName="Variable"
                         suppressId="JBRTest"
                         groupName="JBR"
                         enabledByDefault="true"
                         level="WARNING"
                         implementationClass="StatementInspection"
                         displayName="Add type"/>
    </extensions>
</idea-plugin>
